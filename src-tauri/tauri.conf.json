{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "QiLuo",
  "version": "0.1.0",
  "identifier": "com.qiluo.dev",
  "build": {
    "frontendDist": "../dist-pro",
    "devUrl": "http://localhost:4000",
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build:pro"
  },
  
  "app": {
    "windows": [
      {
        "title": "祺洛",
        "width": 1344,
        "height": 756,
        "resizable": true,
        "fullscreen": false,
        "url":"http://localhost:4000"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
